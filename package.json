{
  "name": "graphql-json-to-sdl",
  "description": "A command line utility for converting a JSON GraphQL schema to GraphQL SDL.",
  "version": "0.2.3",
  "author": " @CDThomas",
  "bin": {
    "graphql-json-to-sdl": "./bin/run"
  },
  "bugs": "https://github.com/CDThomas/graphql-json-to-sdl/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "graphql": "^14.4.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3",
    "@types/graphql": "^14.2.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^10",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8",
    "tslint": "^5",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/CDThomas/graphql-json-to-sdl",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "graphql-json-to-sdl"
  },
  "repository": "CDThomas/graphql-json-to-sdl",
  "scripts": {
